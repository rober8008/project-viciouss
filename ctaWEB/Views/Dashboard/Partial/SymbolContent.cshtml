@model ctaWEB.Models.SymbolContentModel

<style>div.google-visualization-tooltip { width: 150px !important; height: inherit !important;}</style>
<div class="col-md-12">       
    <div id="dashboard" style="margin-top:5px;">
        <div id="combo_chart" class="chart"></div>
        <div id="volume_chart" style="margin-top:-36px;" class="chart"></div>
        <div id="controls" style="margin-top:-19px;margin-bottom:50px;" class="chart"></div>
        <div class="col-lg-12 btn-toolbar" role="toolbar" style="border: 1px solid #E4E4E4;padding:5px;border-radius:5px;margin-top:-45px;text-align:center;">
            <div class="col-lg-offset-4 col-lg-4 btn-group" role="group" style="margin-top:4px;">
                <button id="double_left_arrow" type="button" class="col-lg-3 btn btn-default" style="height:36px;"><img src="~/Images/png/double_left_arrow.png" style="width:100%;height:100%;" /></button>
                <button id="single_left_arrow" type="button" class="col-lg-3 btn btn-default" style="height:36px;"><img src="~/Images/png/single_left_arrow.png" style="width:100%;height:100%;" /></button>
                <button id="single_right_arrow" type="button" class="col-lg-3 btn btn-default" style="height:36px;"><img src="~/Images/png/single_right_arrow.png" style="width:100%;height:100%;" /></button>
                <button id="double_right_arrow" type="button" class="col-lg-3 btn btn-default" style="height:36px;"><img src="~/Images/png/double_right_arrow.png" style="width:100%;height:100%;" /></button>
            </div>
        </div>
        <!--Create elements needded to draw indicators-->
        @foreach (var indicator in Model.Symbol_Dashboard.Indicators)
        {
            if (!indicator.In_Main_Chart)
            {
                <div style="font-size: x-large;">@indicator.DisplayValue</div>
                <div id="@Model.Symbol_Dashboard.Symbol.Symbol_Name.Trim()@indicator.ID" class="chart"></div>
            }
        }   
    </div>
</div>