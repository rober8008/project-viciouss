@{
    ViewBag.Title = "Quotes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container" style="margin-top:20px;">
    <ul class="nav nav-tabs nav-justified">
        <li class="active"><a data-toggle="tab" href="#quotescontainer">Acciones</a></li>
        <li><a data-toggle="tab" href="#bondscontainer">Bonos</a></li>
        <li><a data-toggle="tab" href="#indexescontainer">Indices</a></li>        
    </ul>
    <div class="tab-content">
        <div id="quotescontainer" class="tab-pane fade in active">
            <div id="quotes_content">@Html.Partial("~/Views/Quotes/Partial/Shares.cshtml", null)</div>           
        </div>
        <div id="bondscontainer" class="tab-pane fade in">
            <div id="bonds_content">@Html.Partial("~/Views/Quotes/Partial/Bonds.cshtml")</div>
        </div>
        <div id="indexescontainer" class="tab-pane fade in">
            <div id="indexes_content">@Html.Partial("~/Views/Quotes/Partial/Indexes.cshtml")</div>
        </div>
    </div>
</div>
